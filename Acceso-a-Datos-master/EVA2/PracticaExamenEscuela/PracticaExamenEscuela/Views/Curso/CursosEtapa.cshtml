@model IEnumerable<PracticaExamenEscuela.Models.Cursos>

@{
    ViewBag.Title = "CursosEtapa";
}

<h2>Cursos Etapa</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreCurso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Etapas.Acronimo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreCurso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Etapas.Acronimo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
